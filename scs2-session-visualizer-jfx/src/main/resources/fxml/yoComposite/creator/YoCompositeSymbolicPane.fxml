<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTextField?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox fx:id="mainPane" spacing="10.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="us.ihmc.scs2.sessionVisualizer.jfx.controllers.yoComposite.creator.YoCompositeCreatorWindowController.YoCompositeSymbolicController">
    <children>
        <HBox alignment="CENTER" spacing="5.0" VBox.vgrow="NEVER">
            <children>
                <Label text="YoVariable:"/>
                <JFXTextField fx:id="yoCompositeNameTextField"/>
            </children>
        </HBox>
        <Label alignment="CENTER" contentDisplay="RIGHT" text="Type equation here">
            <graphic>
                <FontAwesomeIconView glyphName="HAND_ALT_DOWN" size="16"/>
            </graphic>
        </Label>
        <TextArea fx:id="yoCompositeEquationTextArea" prefHeight="200.0" prefWidth="200.0"/>
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
</VBox>
