<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXListView?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="mainPane" stylesheets="@../../../css/GeneralStylesheet.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="us.ihmc.scs2.sessionVisualizer.jfx.controllers.yoComposite.creator.YoCompositeAndEquationEditorWindowController">
   <children>
      <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox spacing="5.0" VBox.vgrow="ALWAYS">
               <children>
                  <VBox spacing="10.0">
                     <children>
                        <HBox alignment="CENTER" maxHeight="-Infinity" minHeight="-Infinity" spacing="10.0">
                           <children>
                              <Label maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" text="User YoVariables" />
                              <Separator HBox.hgrow="ALWAYS" />
                              <JFXButton contentDisplay="GRAPHIC_ONLY" focusTraversable="false" graphicTextGap="0.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onAction="#newYoComposite" styleClass="chart-close-button">
                                 <graphic>
                                    <FontAwesomeIconView size="20" styleClass="add-icon-view" />
                                 </graphic>
                              </JFXButton>
                           </children>
                        </HBox>
                        <JFXListView fx:id="yoCompositeListView" prefHeight="250.0" prefWidth="300.0" VBox.vgrow="ALWAYS" />
                        <HBox alignment="CENTER_LEFT" maxHeight="-Infinity" minHeight="-Infinity" spacing="10.0" styleClass="graphic-editor-hbox">
                           <children>
                              <Label maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" text="Equations" />
                              <Separator HBox.hgrow="ALWAYS" />
                              <JFXButton contentDisplay="GRAPHIC_ONLY" focusTraversable="false" graphicTextGap="0.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onAction="#newEquation" styleClass="chart-close-button">
                                 <graphic>
                                    <FontAwesomeIconView size="20" styleClass="add-icon-view" />
                                 </graphic>
                              </JFXButton>
                           </children>
                        </HBox>
                        <JFXListView fx:id="yoEquationEditorListView" prefHeight="250.0" prefWidth="300.0" VBox.vgrow="ALWAYS" />
                     </children>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </VBox>
                  <Separator orientation="VERTICAL" />
                  <VBox fx:id="equationEditorContainer" prefWidth="400.0" spacing="10.0">
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                     <children>
                        <HBox alignment="CENTER" spacing="10.0" VBox.vgrow="NEVER">
                           <children>
                              <Label text="Equation editor" />
                              <Separator maxHeight="-Infinity" minHeight="-Infinity" HBox.hgrow="ALWAYS" />
                              <JFXButton contentDisplay="GRAPHIC_ONLY" focusTraversable="false" graphicTextGap="0.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onAction="#openHelpDialog" styleClass="chart-close-button">
                                 <graphic>
                                    <FontAwesomeIconView size="20" styleClass="help-icon-view" />
                                 </graphic>
                              </JFXButton>
                           </children>
                        </HBox>
                     </children>
                  </VBox>
               </children>
            </HBox>
            <HBox styleClass="graphic-editor-hbox" VBox.vgrow="NEVER">
               <children>
                  <JFXButton fx:id="exportButton" minWidth="-Infinity" onAction="#exportYoEquations" styleClass="graphic-editor-button" text="Export...">
                     <graphic>
                        <FontAwesomeIconView>
                           <styleClass>
                              <String fx:value="graphic-editor-icon-view" />
                              <String fx:value="save-icon-view" />
                           </styleClass>
                        </FontAwesomeIconView>
                     </graphic>
                  </JFXButton>
                  <JFXButton fx:id="importButton" minWidth="-Infinity" onAction="#importYoEquations" styleClass="graphic-editor-button" text="Import...">
                     <graphic>
                        <FontAwesomeIconView>
                           <styleClass>
                              <String fx:value="graphic-editor-icon-view" />
                              <String fx:value="load-icon-view" />
                           </styleClass>
                        </FontAwesomeIconView>
                     </graphic>
                  </JFXButton>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</AnchorPane>
