<?xml version="1.0" ?>
<robot name="CrossFourBarRobot" xmlns:xacro="http://ros.org/wiki/xacro">
  <link name="baseLink">
    <visual>
      <geometry>
        <box size="0 0 0"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </visual>
  </link>
  <link name="endEffectorLink">
    <inertial>
      <mass value="5"/>
      <origin rpy="0 0 0" xyz="0  0  -0.05"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.005"/>
    </inertial>
    <visual>
      <geometry>
        <cylinder radius="0.05" length="0.1"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </visual>
  </link>
  <joint name="crossFourBar" type="cross_four_bar">
    <axis xyz="0 1 0"/>
    <parent link="baseLink"/>
    <child link="endEffectorLink"/>
    <limit effort="190" lower="-1.1" upper="0.4141" velocity="5.89"/>
    <dynamics damping="0.1" friction="0"/>
    <sub_joint>
      <joint name="jointA" type="revolute">
        <origin rpy="0 0 0" xyz="0.01 0 0"/>
        <parent link="baseLink"/>
        <child link="bodyDA"/>
        <limit effort="190" lower="-1.1" upper="0.4141" velocity="5.89"/>
      </joint>
    </sub_joint>
    <sub_joint>
      <joint name="jointB" type="revolute">
        <origin rpy="0 0 0" xyz="-0.01 0 -0.05"/>
        <parent link="baseLink"/>
        <child link="bodyBC"/>
        <limit effort="190" lower="-1.1" upper="0.4141" velocity="5.89"/>
      </joint>
    </sub_joint>
    <sub_joint>
      <joint name="jointC" type="revolute">
        <origin rpy="0 0 0" xyz="0.02 0 -0.05"/>
        <parent link="bodyBC"/>
        <child link="endEffectorLink"/>
        <limit effort="190" lower="-1.1" upper="0.4141" velocity="5.89"/>
      </joint>
    </sub_joint>
    <sub_joint>
      <joint name="jointD" type="revolute">
        <origin rpy="0 0 0" xyz="-0.02 0 -0.05"/>
        <parent link="bodyDA"/>
        <child link="endEffectorLink"/>
        <limit effort="190" lower="-1.1" upper="0.4141" velocity="5.89"/>
      </joint>
    </sub_joint>
    <sub_link>
      <link name="bodyDA">
        <inertial>
          <mass value="0.025"/>
          <origin rpy="0 0.707 0" xyz="-0.01  0  -0.025"/>
          <inertia ixx="1.0e-8" ixy="0" ixz="0" iyy="1.0e-8" iyz="0" izz="1.0e-8"/>
        </inertial>
        <visual>
          <geometry>
            <cylinder radius="0.0125" length="0.05"/>
          </geometry>
          <origin rpy="0 0 0" xyz="0 0 0"/>
        </visual>
      </link>
      <link name="bodyBC">
        <inertial>
          <mass value="0.025"/>
          <origin rpy="0 0 0" xyz="-0.01  0  -0.025"/>
          <inertia ixx="1.0e-8" ixy="0" ixz="0" iyy="1.0e-8" iyz="0" izz="1.0e-8"/>
        </inertial>
        <visual>
          <geometry>
            <cylinder radius="0.0125" length="0.05"/>
          </geometry>
          <origin rpy="0 0 0" xyz="0 0 0"/>
        </visual>
      </link>
    </sub_link>
  </joint>
</robot>
